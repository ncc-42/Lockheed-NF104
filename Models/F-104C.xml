<!--***gear assemblies***-->

	<!--selections-->

<animation>
	<type>select</type>
	<object-name>left-gear-assembly</object-name>
	<object-name>right-gear-assembly</object-name>
	<condition>
		<greater-than>
			<property>gear/gear[1]/position-norm</property>
			<value>.01</value>
		</greater-than>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>door-strut</object-name>
	<condition>
		<equals>
			<property>gear/gear[1]/position-norm</property>
			<value>1</value>
		</equals>
	</condition>
</animation>    

