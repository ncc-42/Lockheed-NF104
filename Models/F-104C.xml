<!--***chute tray-->

<animation>
	<type>rotate</type>
	<object-name>chute-tray</object-name>
	<property>/fdm/jsbsim/systems/chute/deploy</property>
	<factor>30</factor>
	<center>
		<x-m>5.628610</x-m>
		<y-m>0</y-m>
		<z-m>-1.077489</z-m>
	</center>
	<axis>
		<y>1</y>
	</axis>
</animation>

