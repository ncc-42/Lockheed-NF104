<!--***throttle motion***-->

<animation>
	<type>translate</type>
	<object-name>throttle</object-name>
	<property>/fdm/jsbsim/fcs/throttle-pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>-0.0626</dep></entry>
		</interpolation>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
</animation>

<animation>
	<type>translate</type>
	<object-name>throttle</object-name>
	<property>/fdm/jsbsim/fcs/throttle-pos-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>0.0238</dep></entry>
		</interpolation>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>1</z>
		</axis>
</animation>

<!--***gunsight pipper***-->

<animation>
	<type>select</type>
	<object-name>reticle</object-name>
	<condition>   
		<and>
			<greater-than-equals>
				<property>orientation/side-slip-deg</property>  
				<value>-2</value>				 
			</greater-than-equals>
			<less-than-equals>
				<property>orientation/side-slip-deg</property>  
				<value>2</value>				 
			</less-than-equals>
			<greater-than-equals>
				<property>/accelerations/pilot/G</property>  
				<value>-1</value>				 
			</greater-than-equals>
			<less-than-equals>
				<property>/accelerations/pilot/G</property>  
				<value>4</value>				 
			</less-than-equals>
		</and>
	</condition>
</animation>

<animation>
	<type>material</type>
	<object-name>reticle</object-name>
	<emission>
		<red>.7</red>
		<green>1.0</green>
		<blue>0</blue>
		<factor-prop>controls/lighting/gunsight</factor-prop>
	</emission>
</animation>

<animation> 
	<type>translate</type>
	<object-name>reticle</object-name>
	<property>orientation/side-slip-deg</property>
	<interpolation>
		<entry><ind>-2</ind><dep>0.075</dep></entry>
		<entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>2</ind><dep>-0.075</dep></entry>
	</interpolation>
	<axis> 
		<y>1</y>
	</axis>
</animation>

<animation> 
	<type>translate</type>
	<object-name>reticle</object-name>
	<property>/accelerations/pilot/G</property>
	<interpolation>
		<entry><ind>0</ind><dep>0.046</dep></entry>
		<entry><ind>1</ind><dep>0</dep></entry>
		<entry><ind>4</ind><dep>-0.14</dep></entry>
	</interpolation>
	<axis> 
		<z>1</z>
	</axis>
</animation>

<!--***tailhook release***-->

<animation>
	<type>pick</type>
	<object-name>tailhook-button</object-name>
	<visible>true</visible>
	<action>
		<binding>
			<command>property-toggle</command>
			<property>fdm/jsbsim/systems/hook/tailhook-cmd-norm</property>
		</binding>
	</action>
</animation>

<animation>
	<type>select</type>
	<object-name>tailhook-lens-off</object-name>
	<condition>
		<less-than>
			<property>gear/tailhook/position-norm</property>
			<value>1</value>
		</less-than>
	</condition>
</animation>

<animation>
	<type>select</type>
	<object-name>tailhook-lens-on</object-name>
	<condition>
		<equals>
			<property>gear/tailhook/position-norm</property>
			<value>1</value>
		</equals>
	</condition>
</animation>

</PropertyList>

